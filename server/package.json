{
  "name": "server",
  "version": "1.0.0",
  "description": "WebRTC app",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "eslint ./src --fix",
    "build": "npx --package typescript tsc",
    "watch": "npx --package typescript tsc -w",
