  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "concurrently": "^7.2.1",
    "eslint": "^8.17.0",
    "express": "^4.18.1",
    "nodemon": "^2.0.16",
    "socket.io": "^4.5.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.3"
  }
}
